<?xml version="1.0" encoding="utf-8"?>
<layout>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bg_rounded_8_black"
    android:backgroundTint="@color/neutral100"
    android:paddingStart="12dp"
    android:paddingTop="4dp"
    android:paddingEnd="12dp"
    android:paddingBottom="4dp">
    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/TextBodyBoldMedium"
        android:textColor="@color/contentPrimary"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="From"

        />
    <TextView
        android:id="@+id/tvAddress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/TextCaptionRegular"
        android:textColor="@color/contentSecondary"
        app:layout_constraintTop_toBottomOf="@+id/tvTitle"
        android:singleLine="true"
        android:maxLines="1"
        android:ellipsize="middle"
        tools:text="weqweqweqweqwe"

        />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>